{
  "model": {
    "fc_model": {
      "sizes": [96],
      "input_norm": true,
      "activation": "LeakyReLU",
      "dropout": 0.5
    },
    "transformer": {
      "N": 2,
      "d_ff": 192,
      "h": 1,
      "positional_encoding" : null,
      "dropout": 0.5
    },
    "post_model": {
      "output_activation": null,
      "d_output": 1
    }
  },
  "data": { 
    "path": "./expedia_data/0430_60C_norank_propid",
    "validation_ds_role": "test",
    "num_workers": 14,
    "batch_size": 64,
    "slate_length": 30
  },
  "optimizer": {
    "name": "Adam",
    "args": {
      "lr": 0.001
    }
  },
  "lr_scheduler": {
    "name": "StepLR",
    "args": {
      "step_size": 60,
      "gamma": 0.1
    }
  },
  "training": {
    "epochs": 60,
    "early_stopping_patience": 20,
    "gradient_clipping_norm": null
  },
  "val_metric": "ndcg_5",
  "metrics": [
    "ndcg_5",
    "ndcg_10",
    "ndcg_20",
    "ndcg_30"
  ],
  "loss": {
    "name": "approxNDCGLoss",
    "args":
    {
      "alpha": 1.0
    }
  },
  "detect_anomaly": false,
  "expected_metrics" : {
    "val": {
      "ndcg_5": 0.50
    }
  }
}